<tns:implementation xmlns:tns="http://httpconnector.coe.cordys.com/2.0/implementation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://httpconnector.coe.cordys.com/2.0/configuration ../../../../../../../../docs/external/configuration.xsd ">
    <tns:connection-id>JIRA</tns:connection-id>
    <tns:uri>secure/admin/user/AddUser.jspa</tns:uri>
    <tns:clean-response-body>true</tns:clean-response-body>
    <tns:http-method>POST</tns:http-method>
    <tns:request-handler class="com.cordys.coe.ac.httpconnector.rest.html.RestHTMLRequestHandler">
      <tns:parameters>
        <tns:parameter optional="false" destination="request">
          <tns:optional>false</tns:optional>
          <tns:destination>request</tns:destination>
          <tns:name>os_username</tns:name>
          <tns:type>connection-parameter</tns:type>
          <tns:value>jira_username</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>false</tns:optional>
          <tns:destination>request</tns:destination>
          <tns:name>os_password</tns:name>
          <tns:type>connection-parameter</tns:type>
          <tns:value>jira_password</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>false</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>username</tns:name>
          <tns:type>xpath</tns:type>
          <tns:value>ns:username</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>false</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>password</tns:name>
          <tns:type>xpath</tns:type>
          <tns:value>ns:password</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>false</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>confirm</tns:name>
          <tns:type>xpath</tns:type>
          <tns:value>ns:password</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>false</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>fullname</tns:name>
          <tns:type>xpath</tns:type>
          <tns:value>ns:fullname</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>false</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>email</tns:name>
          <tns:type>xpath</tns:type>
          <tns:value>ns:email</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>true</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>sendEmail</tns:name>
          <tns:type>fixed</tns:type>
          <tns:value>true</tns:value>
        </tns:parameter>
        <tns:parameter>
          <tns:optional>true</tns:optional>
          <tns:destination>body</tns:destination>
          <tns:name>Create</tns:name>
          <tns:type>fixed</tns:type>
          <tns:value>Create</tns:value>
        </tns:parameter>
      </tns:parameters>
    </tns:request-handler>
    <tns:response-handler class="com.cordys.coe.ac.httpconnector.samples.JIRAResponseHandler" />
    <tns:valid-response-code>200</tns:valid-response-code>
    <tns:namespaces>
      <tns:binding prefix="ns" uri="http://httpconnector.coe.cordys.com/2.0/REST/JIRA" />
    </tns:namespaces>
  </tns:implementation>